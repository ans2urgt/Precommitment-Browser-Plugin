{
    "name": "Precommitment",
    "version": "1.0.0",
    "description": "This plugin will help you set and remind your precommitments when you visit a website",
    "manifest_version": 3,
    "author": "ans2urgt",
    "action":{
        "default_popup": "src/index.html",
        "default_title": "Precommitment"
    },
    "permissions": [
        "storage",
        "notifications",
        "activeTab", 
        "tabs"
    ],
    "background": {
        "service_worker": "service-worker.js"
    }
}